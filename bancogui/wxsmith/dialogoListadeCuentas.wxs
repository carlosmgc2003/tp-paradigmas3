<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="dialogoListadeCuentas">
		<title>Lista de Cuentas Activas</title>
		<centered>1</centered>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
					<object class="wxBoxSizer" variable="BoxSizer2" member="no">
						<orient>wxVERTICAL</orient>
						<object class="sizeritem">
							<object class="wxChoice" name="ID_CHOICELISTADECUENTAS" variable="ChoiceListadeCuentas" member="yes">
								<content>
									<item>Cuentas Activas</item>
									<item>Cuentas con saldo $0</item>
									<item>Cuentas con saldo negativo</item>
									<item>Cuentas con saldo positivo</item>
								</content>
								<selection>0</selection>
								<handler function="OnChoiceListadeCuentasSelect" entry="EVT_CHOICE" />
							</object>
							<flag>wxEXPAND</flag>
						</object>
						<object class="sizeritem">
							<object class="wxListCtrl" name="ID_LISTCTRLLISTADECUENTAS" variable="ListCtrlListaDeCuentas" member="yes">
								<size>204,270</size>
								<style>wxLC_REPORT</style>
								<handler function="OnListCtrl1BeginDrag" entry="EVT_LIST_BEGIN_DRAG" />
							</object>
							<flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						</object>
						<object class="sizeritem">
							<object class="wxButton" name="ID_BUTTONDIALOGOLISTADECUENTASCERRAR" variable="ButtonDialogoListadeCuentasCerrar" member="yes">
								<label>Cerrar</label>
								<handler function="OnButton1Click" entry="EVT_BUTTON" />
							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>5</border>
							<option>1</option>
						</object>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
