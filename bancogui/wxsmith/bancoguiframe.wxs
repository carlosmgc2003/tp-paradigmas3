<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="bancoguiFrame">
		<title>Banco CrisNaMa</title>
		<icon stock_id="wxART_DELETE" />
		<size>820,500</size>
		<minsize>-1,-1</minsize>
		<maxsize>-1,-1</maxsize>
		<parent_arg>0</parent_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<object class="sizeritem">
				<object class="wxPanel" name="ID_PRINCIPAL" variable="Principal" member="yes">
					<object class="wxBoxSizer" variable="BoxSizer2" member="no">
						<object class="sizeritem">
							<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
								<label>Clientes</label>
								<object class="sizeritem">
									<object class="wxListCtrl" name="ID_LISTACLIENTES" variable="ListaClientes" member="yes">
										<help>Lista de clientes</help>
										<style>wxLC_REPORT</style>
										<handler function="OnListCtrl1BeginDrag1" entry="EVT_LIST_BEGIN_DRAG" />
										<handler function="OnListaClientesItemSelect" entry="EVT_LIST_ITEM_SELECTED" />
									</object>
									<flag>wxALL|wxEXPAND</flag>
									<border>5</border>
									<option>1</option>
								</object>
							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>5</border>
							<option>1</option>
						</object>
						<object class="sizeritem">
							<object class="wxBoxSizer" variable="BoxSizer3" member="no">
								<orient>wxVERTICAL</orient>
								<object class="sizeritem">
									<object class="wxStaticBoxSizer" variable="StaticBoxSizer2" member="no">
										<label>Herramientas</label>
										<orient>wxVERTICAL</orient>
										<object class="sizeritem">
											<object class="wxButton" name="ID_BTNCREARCLIENTE" variable="btnCrearCliente" member="yes">
												<label>Crear Cliente</label>
												<focused>1</focused>
												<tooltip>Abre el dialogo de crear cliente</tooltip>
												<handler function="OnbtnCrearClienteClick" entry="EVT_BUTTON" />
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxButton" name="ID_BTNEDITARCLIENTE" variable="BtnEditarCliente" member="yes">
												<label>Editar Cliente</label>
												<enabled>0</enabled>
												<handler function="OnBtnEditarClienteClick" entry="EVT_BUTTON" />
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxButton" name="ID_BTNELIMINARCLIENTE" variable="BtnEliminarCliente" member="yes">
												<label>Eliminar Cliente</label>
												<enabled>0</enabled>
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
									<flag>wxALL|wxEXPAND</flag>
									<border>1</border>
									<option>1</option>
								</object>
								<object class="sizeritem">
									<object class="wxStaticBoxSizer" variable="StaticBoxSizer3" member="no">
										<label>Estado Bancario del Cliente</label>
										<orient>wxVERTICAL</orient>
										<object class="sizeritem">
											<object class="wxListCtrl" name="ID_LISTACUENTAS" variable="ListaCuentas" member="yes">
												<help>Lista de cuentas de cliente</help>
												<minsize>-1,-1</minsize>
												<maxsize>-1,-1</maxsize>
												<style>wxLC_REPORT</style>
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxBoxSizer" variable="BoxSizer4" member="no">
												<object class="sizeritem">
													<object class="wxStaticBoxSizer" variable="StaticBoxSizer4" member="no">
														<label>Cartera</label>
														<orient>wxVERTICAL</orient>
														<object class="sizeritem">
															<object class="wxButton" name="ID_BUTTON1" variable="ButtonNuevaCuenta" member="yes">
																<label>Nueva Cuenta</label>
																<enabled>0</enabled>
															</object>
															<flag>wxALL|wxEXPAND</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxButton" name="ID_BUTTON2" variable="ButtonCerrarCuenta" member="yes">
																<label>Cerrar Cuenta</label>
																<enabled>0</enabled>
															</object>
															<flag>wxALL|wxEXPAND</flag>
															<border>5</border>
															<option>1</option>
														</object>
													</object>
													<flag>wxALL|wxEXPAND</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxStaticBoxSizer" variable="StaticBoxSizer5" member="no">
														<label>Caja</label>
														<orient>wxVERTICAL</orient>
														<object class="sizeritem">
															<object class="wxButton" name="ID_BUTTON3" variable="ButtonDeposito" member="yes">
																<label>Depósito</label>
																<enabled>0</enabled>
																<font>
																	<size>0</size>
																	<weight>bold</weight>
																</font>
															</object>
															<flag>wxALL|wxEXPAND</flag>
															<border>5</border>
															<option>1</option>
														</object>
														<object class="sizeritem">
															<object class="wxButton" name="ID_BUTTON4" variable="ButtonExtraccion" member="yes">
																<label>Extracción</label>
																<enabled>0</enabled>
																<font>
																	<size>0</size>
																	<weight>bold</weight>
																</font>
															</object>
															<flag>wxALL|wxEXPAND</flag>
															<border>5</border>
															<option>1</option>
														</object>
													</object>
													<flag>wxALL|wxEXPAND</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>1</border>
											<option>1</option>
										</object>
									</object>
									<flag>wxALL|wxEXPAND</flag>
									<border>1</border>
									<option>1</option>
								</object>
							</object>
							<flag>wxALL|wxEXPAND|wxFIXED_MINSIZE</flag>
							<border>5</border>
							<option>1</option>
						</object>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<option>1</option>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;Archivo</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Cerrar</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="yes">
				<label>Cli&amp;entes</label>
				<object class="wxMenuItem" name="ID_MENUITEM1" variable="MenuCrearCliente" member="yes">
					<label>&amp;Crear cliente</label>
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM2" variable="MenuItem4" member="yes">
					<label>E&amp;ditar cliente</label>
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM3" variable="MenuItem5" member="yes">
					<label>&amp;Eliminar cliente</label>
				</object>
			</object>
			<object class="wxMenu" variable="Menu4" member="yes">
				<label>Rep&amp;orte</label>
				<object class="wxMenuItem" name="ID_MENUITEM4" variable="MenuItem6" member="yes">
					<label>Generar...</label>
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>A&amp;yuda</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>Acerca de Banco</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
